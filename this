function drawFlag(x,y){     
  flagPosY=height;
  flagPosX=0;
  if(flagPosY>rectHeight){
    flagPosY=rectHeight;
    flagPosX=rectPosX;
    fill(40,50,178);
    rect(flagPosX,flagPosY,10,10);
  }
  
}
if(y<rectHeight){
    y=rectHeight;
    x=rectPosX;
    fill(40,50,178);
    rect(x,y-=10,2,10);
  }
  drawFlag();
  let flagPosX;
  let flagPosY;